# Prisma
DATABASE_URL="mysql://username:password@host:port/database"

# Google OAuth for Auth.js
AUTH_GOOGLE_ID=your_google_oauth_client_id
AUTH_GOOGLE_SECRET=your_google_oauth_client_secret

# Auth.js setup
AUTH_SECRET='your_auth_secret_key'
NEXTAUTH_SECRET='your_nextauth_secret'
NEXTAUTH_URL='http://localhost:3000'

# Aeries connection
DB_USER=your_db_username
DB_PASSWORD=your_db_password
DB_SERVER=your_db_server
DB_DATABASE=your_database_name

# Aeries API
AERIES_API_KEY=your_aeries_api_key
NEXT_PUBLIC_AERIES_URL=https://your-aeries-instance.domain.com/Aeries.Net

# Changing queries
QUERY_ASSESSMENT_GRADE_PERCENTAGE=your_query_id

# AG Grid License
NEXT_PUBLIC_AG_GRID_LICENSE_KEY=your_ag_grid_license_key

# FastAPI Server
NEXT_PUBLIC_FAST_API_URL='http://your-fastapi-server:port'
NEXT_PUBLIC_FAST_API_USER='fastAPI_username'
NEXT_PUBLIC_FAST_API_PASSWORD='fastAPI_password'

# AI Query Builder - LLM Configuration
# Option 1: Local LLM (Ollama) - for development
LLM_PROVIDER=local
LLM_BASE_URL=http://localhost:11434/v1
LLM_MODEL=llama3.2

# Option 2: Google Gemini - for production
# LLM_PROVIDER=gemini
# LLM_API_KEY=your_gemini_api_key
# LLM_MODEL=gemini-2.0-flash-exp

# Option 3: OpenAI
# LLM_PROVIDER=openai
# LLM_API_KEY=sk-your_openai_key
# LLM_MODEL=gpt-4o-mini

# Common LLM settings
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=2000