[
  {
    "id": "students_base",
    "name": "Active Students Base Query",
    "description": "Base query for currently enrolled students with basic demographics",
    "snippet": "SELECT s.ID as student_id, s.SN as student_number, s.LN as last_name, s.FN as first_name, s.GR as grade, s.SC as school_code, s.BD as birth_date, s.SX as gender, s.EC as ethnicity, s.SEM as student_email, s.HL as home_language, s.CU as class_teacher_num FROM STU s WHERE s.DEL = 0 AND s.TG = ''",
    "type": "base",
    "category": "base_queries",
    "subcategory": "students",
    "tags": [
      "students",
      "active",
      "enrolled",
      "demographics",
      "base"
    ],
    "tables": [
      "STU"
    ],
    "dependencies": [],
    "conflicts": [
      "staff_base",
      "transcripts_base"
    ],
    "parameters": [],
    "outputColumns": [
      "student_id",
      "student_number",
      "last_name",
      "first_name",
      "grade",
      "school_code",
      "birth_date",
      "gender",
      "ethnicity",
      "student_email",
      "home_language",
      "class_teacher_num"
    ]
  },
